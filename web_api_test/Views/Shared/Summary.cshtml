@using web_api_test.Models;
@model IEnumerable<IReservation>


<div class="panel-heading">Все заказы</div>
<div class="panel-body">
    <table class="table table-striped table-condensed">
        <thead>
            <tr><th>ID</th><th>Имя</th><th>Помещение</th><th>Почта</th><th>Телефон</th><th></th></tr>
        </thead>
        <tbody data-bind="foreach: model.reservations">
            <tr>
                <td data-bind="text: ReservationId"></td>
                <td data-bind="text: ClientName"></td>
                <td data-bind="text: Location"></td>
                <td data-bind="text: Email"></td>
                <td data-bind="text: Phone"></td>
                <td>
                    <button class="btn btn-xs btn-primary"
                            data-bind="click: removeItem">
                        Удалить
                    </button>
                </td>
            </tr>
        </tbody>
    </table>
</div>